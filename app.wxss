/**index.wxss 原创首页**/
.loading{
  padding: 20rpx 0;
  background: black;
}
/* 没有更多 */
.no_more_tips{
  font-size: 22rpx;
  line-height: 1;
  text-align: center;
  padding: 22rpx 0;
  color:#656565;
}
.section {
  background: #fff;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  box-sizing: border-box;
} 
/* 首页模块-标题  */
.sec_tit{
  font: 30rpx/1 "Microsoft YaHei";
  text-align: left;
  padding:40rpx 38rpx 30rpx;
}
/* 首页模块-列表 */
.sec_list{
  border-bottom: 18rpx solid #f5f5f5;
}
.sec_list:last-child{
  border-bottom:none;
}
.sec_list_item{
  padding:30rpx 38rpx;
  border-bottom: 1px solid #eaeaea;
  display: flex;
  flex-direction: row;
}
/* 缩略图 */
.item_bookpic{
  width: 153rpx;
  height: 205rpx;
  margin-right: 25rpx;
  border:1px solid #eaeaea;
  /* 默认封面 */
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100% 100%;
}
.item_bookpic image{
  width: 100%;
  height: 100%;
}
/* 书卡信息 */
.item_bookintro_box{
  width:495rpx;
  font:24rpx/1 'Microsoft YaHei';
  color:#656565;
  line-height:34rpx;
  display:-webkit-box;
  -webkit-box-orient:vertical;
  -webkit-line-clamp:2;
  overflow:hidden;
}
.item_booktit{
  font-size: 30rpx;
  color: #0d0d0d;
  margin-top: 6rpx;
}
.item_bookauthor{
  margin:24rpx 0 46rpx;
  font:24rpx/1 'Microsofty YaHei';
  color: #656565;
  overflow: hidden; 
}
.item_authorname{
  float: left;
  max-width: 50%; 
  line-height: 32rpx;
  display: inline-block;
  overflow:hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.item_booktag{
  float: right; 
  max-width: 45%; 
  padding: 4rpx 10rpx;
  color: #ff2039;
  border:1px solid #ff2c44;
  font-size: 22rpx;
  line-height: 1;
  border-radius: 20rpx;
  overflow:hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  vertical-align: middle;
}
.item_bookintro{
  font:24rpx/1 'Microsoft YaHei';
  color: #656565;
  line-height: 34rpx;
  max-height: 102rpx;
  margin-top: 10rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  text-overflow: ellipsis;
}
.item_videointro_box .item_bookintro{
  -webkit-line-clamp: 3;
}
/* 全部原创按钮 */
.sec_more{
  background: #fff;
  font-size: 26rpx;
  color: #737373;
  line-height: 1;
  padding: 30rpx 0;
  text-align: center;
}
/* 底部菜单栏 */
.menu_place{
  padding-top: 18rpx;
  height: 108rpx;
}
.menu_bar{
  position: fixed;
  z-index: 2;
  bottom: 0;
  left: 0;
  right: 0;
  background: #fff;
  display: flex;
  padding: 18rpx 0;
  flex-direction: row;
}
.menu_bar .menu_item{
  width: 100%;
  text-align: center;
  font-size: 22rpx;
  line-height: 1;
  color: #b2b2b2;
}
/* 视频列表 */
.item_videopic{
  width: 242rpx;
  height: 181rpx;
  margin-right: 25rpx;
  /* 视频无图默认图 */
  background: url(data:image/gif;base64,) no-repeat;
  background-size: 100% 100%;
}
.item_videotit{
  font-size: 30rpx;
  line-height: 38rpx;
  color: #0d0d0d;
  height: 76rpx;
  display:-webkit-box;
  overflow: hidden;
  -webkit-box-orient:vertical;
  -webkit-line-clamp:2;
  text-overflow:ellipsis;
}
.item_videopic image{
  width: 100%;
  height: 100%;
}
.item_videointro_box {
  width: 405rpx;
}

/* 书圈列表 */
.item_circle_read{
  float: right
}
