/* 富文本解析样式 */
@import "/wxParse/wxParse.wxss";

/* 单本原创图书正文 */
page{
  min-height: 100%;
  background: #f6f6ee;
}
.chapter_read{
  background: #f6f6ee;
  padding:0 42rpx;
  min-height: 100%;
  color: #363636;
}
/* 文章标题 */
.chapter_tit{
  font-size: 36rpx;
  color: #fb4c49;
  padding: 42rpx 0;
}
/* 文章正文 */
.chapter_text{
  font-size: 34rpx;
  line-height: 68rpx;
  text-indent: 68rpx;
  text-align: justify;
}
/* 底部操作按钮 */
.menu_btn_prevChap,.menu_btn_ArticleList,.menu_btn_nextChap{
  width: 33.333%;
  display: inline-block;
  font-size: 30rpx;
  line-height: 1;
  padding: 21rpx 0;
  text-align: center;
}
.menu_btn_prevChap.more,.menu_btn_nextChap.more{
  color: #fe2a28;
}

/*mask*/
.toast_mask {
 opacity: 0;
 width: 100%;
 height: 100%;
 overflow: hidden;
 position: fixed;
 top: 0;
 left: 0;
 z-index: 888;
}
/*toast*/
.toast_content_box {
 display: flex;
 width: 100%;
 height: 100%;
 justify-content: center;
 align-items: center;
 position: fixed;
 z-index: 999;
 transition: all 0.5s;
}
.toast_content {
 padding: 20rpx 40rpx;
 background: rgba(0, 0, 0, 0.5);
 border-radius: 20rpx;
}
.toast_content_text {
 height: 100%;
 width: 100%;
 color: #fff;
 font-size: 28rpx;
 text-align: center;
}

/* 日夜模式切换 */
.op_mode_cut{
  position: fixed;
  right: 40rpx;
  bottom: 140rpx;
  width: 94rpx;
  height: 94rpx;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 94rpx 94rpx;
}

.mode_night .chapter_tit{
  color: #fff;
}
.mode_night .op_mode_cut{
  background:url(data:image/png;base64,) no-repeat;
  background-size: 94rpx 94rpx;
}

.mode_night .chapter_read{
  background: #29414a;
  color: #9caab2;
}
.mode_night .menu_place{
  background: #29414a;
}
.mode_night .menu_bar{
  background: #242424;
}
.mode_night .menu_bar view[class^="menu_btn"]{
  color: #eeeeee;
}