<!-- wetoast.wxml -->
<template name="comm_toast_box">
    <view class="toast_mask" wx:if="{{commToast.isShowToast}}"></view>
    <!--以下为toast显示的内容-->
    <view class="toast_content_box" wx:if="{{commToast.isShowToast}}">
      <view class="toast_content">
        <view class="toast_content_text">
          {{commToast.toastText}}
        </view>
      </view>
    </view>
</template>